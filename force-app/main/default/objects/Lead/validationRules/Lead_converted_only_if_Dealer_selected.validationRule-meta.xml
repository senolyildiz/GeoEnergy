<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Lead_converted_only_if_Dealer_selected</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
    IsConverted,
    
     ISBLANK( Dealer__c )
    )</errorConditionFormula>
    <errorMessage>Lead can be converted only if Dealer is selected!!!</errorMessage>
</ValidationRule>
