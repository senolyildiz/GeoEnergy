<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Person_Acc_Com_Channels_Validation</fullName>
    <active>true</active>
    <errorConditionFormula>And(
    PersonDoNotCall,
    PersonHasOptedOutOfEmail 
   )</errorConditionFormula>
    <errorDisplayField>PersonDoNotCall</errorDisplayField>
    <errorMessage>Do Not Call and Email Opt Out field can not be selected together!!!!</errorMessage>
</ValidationRule>
